{
  "name": "zazu-timetracker",
  "version": "0.1.0",
  "description": "Simple zazu timetracker",
  "blocks": {
    "input": [
      {
        "id": "timetrack start",
        "type": "PrefixScript",
        "prefix": ":start",
        "space": false,
        "args": "Optional",
        "script": "src/start.js",
        "connections": ["RegisterStart"]
      }, {
        "id": "timetrack stop",
        "type": "PrefixScript",
        "prefix": ":stop",
        "space": false,
        "args": "Optional",
        "script": "src/stop.js",
        "connections": ["RegisterStop"]
      }, {
        "id": "timetrack history",
        "type": "PrefixScript",
        "prefix": ":hist",
        "space": false,
        "args": "Optional",
        "script": "src/history.js"
      }
    ],
    "output": [
      {
        "id": "RegisterStart",
        "type": "UserScript",
        "script": "src/registerStart.js"
      }, {
        "id": "RegisterStop",
        "type": "UserScript",
        "script": "src/registerStop.js"
      }
    ]
  }
}
